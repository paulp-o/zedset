    "appearance": {
      "title": "Appearance",
      "description": "Visual theme and icon settings",
      "order": 1,
      "paths": [
        "theme",
        "icon_theme"
      ]
    },
    "editor": {
      "title": "Editor",
      "description": "Text editing behavior and display settings",
      "order": 2,
      "paths": [
        "base_keymap",
        "vim_mode",
        "relative_line_numbers",
        "cursor_blink",
        "seed_search_query_from_cursor",
        "use_on_type_format",
        "formatter",
        "format_on_save",
        "code_actions_on_format",
        "remove_trailing_whitespace_on_save",
        "ensure_final_newline_on_save"
      ]
    },
    "fonts": {
      "title": "Fonts & Typography",
      "description": "Font settings for editor and UI",
      "order": 3,
      "paths": [
        "buffer_font_family",
        "buffer_font_fallbacks",
        "buffer_font_features",
        "buffer_font_size",
        "buffer_font_weight",
        "buffer_line_height",
        "ui_font_family",
        "ui_font_size",
        "ui_font_weight"
      ]
    },
    "formatting": {
      "title": "Text Formatting",
      "description": "Indentation, wrapping, and whitespace settings",
      "order": 4,
      "paths": [
        "tab_size",
        "hard_tabs",
        "soft_wrap",
        "preferred_line_length",
        "show_whitespaces",
        "wrap_guides"
      ]
    },
    "ui_elements": {
      "title": "UI Elements",
      "description": "Interface components and visual aids",
      "order": 5,
      "paths": [
        "scrollbar",
        "gutter",
        "indent_guides",
        "hover_popover_enabled",
        "inlay_hints"
      ]
    },
    "search": {
      "title": "Search & Find",
      "description": "Search behavior and options",
      "order": 6,
      "paths": [
        "search"
      ]
    },
    "terminal": {
      "title": "Terminal",
      "description": "Integrated terminal configuration",
      "order": 7,
      "paths": [
        "terminal"
      ]
    },
    "panels": {
      "title": "Panels & Docks",
      "description": "Side panel and dock configurations",
      "order": 8,
      "paths": [
        "project_panel",
        "collaboration_panel",
        "chat_panel",
        "notification_panel",
        "outline_panel"
