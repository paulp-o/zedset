      "title": "System & Updates",
      "description": "Application behavior and system integration",
      "order": 12,
      "paths": [
        "auto_update",
        "telemetry",
        "profiles",
        "log"
      ]
    },
    "project": {
      "title": "Project Settings",
      "description": "Project-specific configuration",
      "order": 13,
      "paths": [
        "project_name"
      ]
    }
